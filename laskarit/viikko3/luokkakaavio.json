		<div id="content">

		<script type="text/javascript" charset="utf-8">
  var timeout = null;
  var justLoaded = true;
  var type = 'class';
  var customizations = new Array();
  var theme = 'plain';
  var dsl = "[Pelilauta]-2[Noppa], [Pelilauta]-2...8[Pelaaja], [Ruutu|seuraava: Ruutu]-[Pelilauta], [Ruutu]-[Pelinappula], [Pelinappula]-[Pelaaja], [Aloitusruutu]-&gt;[Ruutu]";
  var extension = '.json';
  var lastHash = null;
  var host_name = 'http://yuml.me/'
  var namespace = ''
  var diagramName = ''
  var digest = '6e4b9a0a'
  var output_without_extension;
  var edit_link,draw_url,short_url_link;
  $(document).ready(function() {
  $('.btn.' + type).removeClass('btn-gray').addClass('btn-blue')
    $(".rss-popup span").click(function() {
        $(this).next("div").stop(true, true).animate({
          opacity: "show",
          top: "-1em"
        }, "fast");
      }
      /*, function() {
      		$(this).next("div").animate({opacity: "hide", top: "20"}, "fast");
      	}*/
    );

    $(".rss-popup-div").hover(null, function() {
      $(".rss-popup-div").animate({
        opacity: "hide",
        top: "-2em"
      }, "fast")
    });

    $('#dsl_text').attr('value', dsl);

    var pressed = {};
    $("#dsl_text").keydown(function(e) {
      pressed[event.keyCode] = true;
      // Check if 'E' was pressed
      if (69 == event.keyCode) {
        // Check if Ctrl + S (Windows) or Command + S (Mac) was pressed
        if (pressed[17] || pressed[224]) {

          buildUrl();
          createDiagram()
          event.preventDefault();
        }
      } else {
        if (timeout) clearTimeout(timeout);
        timeout = setTimeout(function() {
          buildUrl();
          createDiagram()
        }, 800);
      }
    });

    $("#dsl_text").keyup(function(e) {
      delete pressed[e.keyCode];
    });

    $('#usecaseImg').load(function() {
      $('#ajax-loader').hide();
      if (!$.browser.msie) {
        $('#usecaseImg').css({
          opacity: 1
        });
      }
    });

  });


  if (extension == '.png') {
    extension == '';
  }



  var labels = new Array();
  labels['usecase'] = 'Use Case Diagram';
  labels['class'] = 'Class Diagram';
  labels['activity'] = 'Activity Diagram';
  labels['dot'] = 'Dot Diagram';
  labels['mindmap'] = 'MindMap Diagram';

  var examples = new Array()

  examples['activity'] = "(start)->|a|,|a|->(Make Coffee)->|b|,|a|->(Make Breakfast)->|b|,|b|-><c>[want more coffee]->(Make Coffee),<c>[satisfied]->(end)";
  examples['class'] = '// Cool Class Diagram\n[Customer|-forname:string;surname:string|doShiz()]<>-orders*>[Order]\n[Order]++-0..*>[LineItem]\n[Order]-[note:Aggregate root{bg:wheat}]';
  examples['usecase'] = '[Customer]-(Sign In)\n[Customer]-(Buy Products)\n(Buy Products)>(Browse Products)\n(Buy Products)>(Checkout)\n(Checkout)<(Add New Credit Card)';
  examples['dot'] = 'digraph G{\n Hello->World;\n}';
  examples['mindmap'] = '(yUML)-(Drawing)/(Sketching)/(Fun)';


  function createDiagram(){

    if ($('#usecaseImg').attr('src') != encodeURICustom(draw_url)) {
      $('#usecaseImg').css({
        opacity: 0.5
      });
     $('#ajax-loader').show();
       $.ajax({
        method: "POST",
        url: draw_url,
        data: {
          dsl_text: dsl,
          name : diagramName
        },
        success: function(src){
          if (diagramName){
                splitted = $('#usecaseImg').attr('src').split('?')
                  new_url = splitted[0] + '?' + new Date().getTime();
                  $('#usecaseImg').attr('src',  new_url);
          }else{
            $('#usecaseImg').attr('src', '/' + src);
            name = src.replace(/^\/|\.pdf|\.png|.\jpeg/g, '');
            digest = name
            changeImageNames(name)
          }
        }
      });

    }
    return false;
  }

  function getDiagramNameIfEmpty() {
    $.post(encodeURI(short_url_link), function(data) {

        //override GET data (which is garbage because there is no short_url_link for POST, we'd need to make it compatible)
        data = data.replace(/^\/|\.pdf|\.png|.\jpeg/g, '');
        // if (digest){
        //   data = digest;
        // }else{
        //   digest = data
        // }
        // if (diagramName){
        //   data = diagramName
        // }
        // if the hash is the same, then a new diagram hasn't been generated
        // so this isn't a new advert view
        if (lastHash != data) {
          $('#advert-iframe').attr('src', '/diagram/google_ad')
          lastHash = data;
        }
        changeImageNames(data)
        $('.twitter_link').text("Tweet this diagram.");
        $('.twitter_link').attr("href", "http://twitter.com/home?status=Check out my @yuml_app diagram http://yuml.me/edit/" + data);

      })
  }

  function buildUrl() {

    if (dsl == '' || justLoaded == false) {
      dsl = $('#dsl_text').attr('value');
    }

    if (type != 'dot') dsl = cleanDsl(dsl)
    var method = dsl.length > 200 ? 'POST' : 'GET';

    //always post dot files, it's simpler
    if (type == 'dot') method = 'POST';

    var template = method == 'GET' ? namespace + "/diagram/[THEME];[CUSTOMISATIONS]/[TYPE]/short_url/[DSL][EXTENSION]" : namespace + "/diagram/[THEME];[CUSTOMISATIONS]/[TYPE]/";
    var output = template.replace('[THEME]', theme);
    output = output.replace('[CUSTOMISATIONS]', getCustomizations());
    output = output.replace('[TYPE]', type);
    output = output.replace('[DSL]', dsl);
    output = output.replace(';/', '/');
    short_url_link = output.replace('[EXTENSION]', extension);
    output_without_extension = output.replace('/short_url/', '/').replace('[EXTENSION]', '')
    edit_link = output.replace('/short_url/', '/edit/').replace('[EXTENSION]', '')
    output = output.replace('[EXTENSION]', extension);
    output = output.replace('/short_url/', '/');

    output = output.replace('[EXTENSION]', extension);
    console.log(output)
    draw_url = output;
      changeEditableLinks()
      console.log(encodeURI(short_url_link))

  }

  function changeEditableLinks() {
      nspace = ""
      dsl_part = output_without_extension.substr(1)

      $('.png_link').text(host_name +  dsl_part);
      $('.png_link').attr("href", host_name +   dsl_part);

      $('#png_link').val(host_name +  dsl_part);
      $('#pdf_link').val(host_name +  dsl_part + '.pdf');
      $('#jpg_link').val(host_name +  dsl_part + '.jpg');
      $('#json_link').val(host_name + dsl_part + '.json');
      $('#svg_link').val(host_name +  dsl_part + '.svg');
      $('#edit_link').val(host_name +  edit_link.substr(1));
  }

  function changeImageNames(name) {
       nspace = ""
        if (namespace != "") {
          nspace = namespace.replace(/\//g, '')
          nspace = nspace + "/"
        }
        $('.png_short_link').text(host_name + nspace + name + '.png');
        $('.png_short_link').attr("href", host_name + nspace + name + '.png');

        $('.pdf_short_link').text(host_name + nspace + name + '.pdf');
        $('.pdf_short_link').attr("href", host_name + nspace + name + '.pdf');

        $('.jpg_short_link').text(host_name + nspace + name + '.jpg');
        $('.jpg_short_link').attr("href", host_name + nspace + name + '.jpg');

        $('.json_short_link').text(host_name + nspace + name + '.json');
        $('.json_short_link').attr("href", host_name + nspace + name + '.json');

        $('.svg_short_link').text(host_name + nspace + name + '.svg');
        $('.svg_short_link').attr("href", host_name + nspace + name + '.svg');

        $('.edit_short_link').text(host_name + nspace + "edit/" + name);
        $('.edit_short_link').attr("href", host_name + nspace + "edit/" + name);
  }

  function saveName() {
    if ($('#diagramName').val().trim() == ''){
      alert('Diagram name can\'t be empty')
      return;
    }
    var pattern = /^[\w&.-]+$/
    if (!pattern.test($('#diagramName').val())){
        alert('Diagram names should only have alpha numerical and these 2 _ - special chracters')
        return;
    }
    $('#saveBtn').attr('disabled',true);
    $.ajax({
          method: 'post',
          url:  namespace + '/name_diagram/' + digest ,
          data: {name:$('#diagramName').val(),old_name:diagramName},
          success: function(data){
            $('#saveBtn').removeAttr('disabled');
            if (data.status == 1){
              alert('Name already taken')
            }else if(data.status == 2){
              alert('You are only allowed to create 5 named diagrams. Please subscribe to pro plan and create more')
            }else{
                diagramName = $('#diagramName').val()
                changeImageNames(diagramName);
                // window.history.pushState("yUML", "yUML", $('.edit_short_link').attr('href'))
                window.location.href = $('.edit_short_link').attr('href')
            }
          }
        })
  }

  function trim(str) {
    return str.substring(0, str.length - 1);
  }

  function encodeURICustom(dsl) {
    tmp = encodeURI(dsl);
    tmp = tmp.replace(/#/g, '%23')
    return tmp;
  }

  function cleanDsl(dsl) {
    tmp = dsl
    tmp = tmp.replace(/\//g, '%2F');
    tmp = tmp.replace(/(\n|\r|\t)+$/, '');
    tmp = tmp.replace(/\n/g, ', ').replace(/,,/g, ',').replace(/, $/, '')
    return tmp;
  }

  function prettyDsl(dsl) {
    tmp = dsl
    tmp = dsl.replace(/\,\s+/g, '\n');
    tmp = tmp.replace(/,,/g, ',').replace(/, $/, '')
    return tmp;
  }

  function customize(property, val) {
    $('.rss-popup-div').hide();
    customizations[property] = val;
    if (val == null) {
      delete(customizations[property])
    }
    buildUrl();
    createDiagram()
  }

  function getCustomizations() {
    var str = "";
    if (customizations['dir']) {
      str += "dir:" + customizations['dir'] + ";";
    }
    if (customizations['scale']) {
      str += "scale:" + customizations['scale'] + ";";
    }
    return str;
  }


  function changeDiagramType(title, typeName, themeName, fileType, fileLabel, dslText) {
    type = typeName || type;
    theme = themeName || theme;
    dsl = dslText || dsl;

    $('.rss-popup-div').hide();
    if (fileType) {
      extension = fileType;
      $('#extension-label').html(fileLabel)
    }
    if (typeName) {
      $('#diagram-label').html(labels[typeName]);
    }
    if (theme) {
      $('#theme-label').html(theme.replace(/nofunky/, 'boring'));
    }
    if (typeName && typeName != 'dot')
      $('#dsl_text').val(prettyDsl(dsl));
    else
      $('#dsl_text').val(dsl);

    buildUrl();
    createDiagram()
  }

  $(document).ready(function() {
    if (dsl == '') {
      dsl = examples[type]
    }

    $('#diagramStyleChangeImage').on('click', function (e) {
      changeDiagramType(null, null, $(this).data('switchTo'));
    })

    if (diagramName){
       output_without_extension = dsl.replace(/^\/|\.pdf|\.png|.\jpeg/g, '');
       $('#diagramName').val(diagramName)
    }
    buildUrl();
    if (digest) {
      name = diagramName || digest
      changeImageNames(name)
      changeEditableLinks()
    }else{
       createDiagram()
       getDiagramNameIfEmpty()
       changeDiagramType(type, type, theme, '', extension)
    }
   $('#usecaseImg').css({
          opacity: 1
        });
   $('#dsl_text').attr('value', dsl);
    justLoaded = false;
  });
</script>
